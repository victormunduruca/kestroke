package model;

public class Tecla {
	private int keycode;
	private double pressionada;
	private double solta;
	
	public Tecla(int k, double p, double s) {
		keycode = k;
		pressionada = p;
		solta = s;
	}

	public int getKeycode() {
		return keycode;
	}

	public void setKeycode(int keycode) {
		this.keycode = keycode;
	}

	public double getPressionada() {
		return pressionada;
	}

	public void setPressionada(double pressionada) {
		this.pressionada = pressionada;
	}

	public double getSolta() {
		return solta;
	}

	public void setSolta(double solta) {
		this.solta = solta;
	}
	
}
